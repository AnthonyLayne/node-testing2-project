{
  "name": "node-testing2-project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=testing jest --runInBand",
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "migrate-down": "knex migrate:down",
    "rollback": "knex migrate:rollback",
