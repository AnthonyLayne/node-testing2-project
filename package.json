    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/node-testing2-project.git"
  },
  "dependencies": {
    "@vscode/sqlite3": "^5.0.8",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "knex": "^2.1.0",
    "knex-cleaner": "^1.3.1",
    "sqlite3": "^5.0.8"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3"
  }
}
